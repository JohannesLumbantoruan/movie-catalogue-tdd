{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "max-len": "off",
    "eol-last": "off",
    "indent": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-prototype-builtins": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": "off"
  }
}
